<h1>Halloween Movies</h1>
<h4><%= link_to 'Alphabetized List', halloween_movies_alphabetical_path %></h4>
<table>
  <% @halloween_movies.each do |halloween_movie| %>
    <tr>
      <th><%= link_to halloween_movie.title, halloween_movie_path(halloween_movie.id) %></th>
      <th><%= image_tag(url_for(halloween_movie.image), style: 'height:10vw;width:7vw') %></th>
    </tr>
  <% end %>
</table>
